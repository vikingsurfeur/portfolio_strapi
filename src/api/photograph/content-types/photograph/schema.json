{
  "kind": "collectionType",
  "collectionName": "photographs",
  "info": {
    "singularName": "photograph",
    "pluralName": "photographs",
    "displayName": "Photograph",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "photographedAt": {
      "type": "date"
    },
    "location": {
      "type": "richtext"
    },
    "isLastWorkPortfolio": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "isOnSale": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "sellerUrl": {
      "type": "string"
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "portfolio": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::portfolio.portfolio",
      "inversedBy": "photographs"
    },
    "format": {
      "type": "enumeration",
      "enum": [
        "vertical",
        "horizontal"
      ]
    }
  }
}
